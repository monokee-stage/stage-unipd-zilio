      <script><![CDATA[var val = execution.getVariable("optionVariable");
java.lang.System.out.println(val);]]></script>
    </scriptTask>
    <exclusiveGateway id="sid-3249DA66-879F-4263-8139-EE2D7BD21A32"></exclusiveGateway>
